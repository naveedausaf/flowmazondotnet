{
  "name": "flowmazonfrontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prettier --check . && next build",
    "build:local": "prettier --write . && next build",
    "format": "prettier --write .",
    "start": "next start",
    "lint": "next lint",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "chromatic": "# do nothing as we don't want Chromatic project token in here",
    "test-storybook": "test-storybook",
    "test-storybook:test": "wait 3000 && echo Starting Storybook tests... && (test-storybook --coverage || true) && echo Completed Storybook tests...",
    "test-storybook:watch": "concurrently --names \"STORYBOOK,TEST\" --hide HTTP --prefix-colors \"blue,magenta\" \"storybook dev -p 6006\" \"wait-on http://127.0.0.1:6006 && wait 10000 && onchange -i -k src/**/* -- npm run test-storybook:test\"",
    "test-playwright:test": "wait-on -d 4000 http-get://localhost:4000/add-product http-get://localhost:5000/health/ready && (playwright test --project chromium firefox webkit || true) && echo Test run report available at http://localhost:6007",
    "test-playwright:watch": "concurrently --names \"REPORT,COMPOSE,TEST\" --prefix-colors \"blue,green,magenta\" \"http-server playwright-report -s --port 6007\" \"cross-env http_proxy=127.0.0.1:8888 https_proxy=127.0.0.1:8888 && docker compose -f ../compose.yaml --env-file ../compose.env up --watch\" \"onchange -i -k  src/**/* e2etests/**/* ../flowmazonbackend/flowmazonapi/**/*.cs ../flowmazonbackend/flowmazonapi/**/*.csproj -- npm run test-playwright:test\""
  },
  "dependencies": {
    "@grafana/faro-web-sdk": "^1.19.0",
    "@grafana/faro-web-tracing": "^1.19.0",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.52.0",
    "@opentelemetry/instrumentation": "^0.52.0",
    "@opentelemetry/resources": "^1.25.1",
    "@opentelemetry/sdk-logs": "^0.52.0",
    "@opentelemetry/sdk-node": "^0.203.0",
    "@opentelemetry/sdk-trace-node": "^1.25.1",
    "@opentelemetry/semantic-conventions": "^1.36.0",
    "@vercel/otel": "^1.13.0",
    "clsx": "^2.1.1",
    "formik": "^2.4.6",
    "next": "^15.3.4",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-error-boundary": "^6.0.0",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.0.1",
    "@playwright/test": "^1.52.0",
    "@storybook/addon-a11y": "9.0.12",
    "@storybook/addon-coverage": "^2.0.0",
    "@storybook/addon-docs": "9.0.12",
    "@storybook/addon-links": "9.0.12",
    "@storybook/addon-onboarding": "9.0.12",
    "@storybook/addon-vitest": "9.0.12",
    "@storybook/nextjs": "9.0.12",
    "@storybook/nextjs-vite": "9.0.12",
    "@storybook/test-runner": "^0.23.0",
    "@swc/helpers": "^0.5.17",
    "@tailwindcss/postcss": "^4.1.4",
    "@types/node": "^24",
    "@types/react": "19.1.2",
    "@types/react-dom": "19.1.2",
    "@types/sinonjs__fake-timers": "^8.1.5",
    "@typescript-eslint/eslint-plugin": "^8.32.0",
    "@typescript-eslint/parser": "^8.32.0",
    "@vitest/browser": "^3.0.9",
    "@vitest/coverage-v8": "^3.0.9",
    "axe-playwright": "^2.1.0",
    "chai": "^5.2.0",
    "chromatic": "^13.0.1",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "daisyui": "5.0.22",
    "dotenv": "^16.5.0",
    "eslint": "^9.24.0",
    "eslint-config-next": "^15.3.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-storybook": "9.0.12",
    "http-server": "^14.1.1",
    "jest-axe": "^10.0.0",
    "msw": "^2.8.2",
    "msw-storybook-addon": "^2.0.4",
    "onchange": "^7.1.0",
    "playwright": "^1.51.1",
    "postcss": "^8",
    "prettier": "^3.6.0",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "storybook": "9.0.18",
    "tailwindcss": "^4.1.4",
    "typescript": "^5.6.3",
    "vitest": "^3.0.9",
    "wait": "^0.4.2",
    "wait-on": "^8.0.3"
  },
  "overrides": {
    "@types/react": "19.1.2",
    "@types/react-dom": "19.1.2",
    "storybook": "$storybook"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
